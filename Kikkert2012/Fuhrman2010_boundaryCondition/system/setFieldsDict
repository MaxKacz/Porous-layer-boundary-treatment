FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (0 0 -1) (15 0.062 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
    
    boxToCell
    {
        box (0 0 -1) (0.983 0.59 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


